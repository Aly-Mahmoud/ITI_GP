/**
  ******************************************************************************
  * @file           : Controlling.c
  * @brief          : Module That Responsible To Control Steering and DC Motor
  * 				  for RC Car.
  ******************************************************************************/
#include "stm32f4xx_hal.h"
#include "cmsis_os2.h"



extern TIM_HandleTypeDef htim1;

void controllingSM(void)
{

}

void Controlling()
{
	while(1)
	{
		HAL_TIM_PWM_Start(&htim1, TIM_CHANNEL_1);
	}
}
